<script src="https://code.jquery.com/jquery-3.5.1.min.js"></script>
<script>
  $(document).ready(function () {
    $('h5.tab__tile').click(function () {
      $('h5.tab__tile').removeClass('active_heading');
      $(this).addClass('active_heading');
      $('.tabbing__list__area').hide();
      var show__cat = $(this).data('targe');
      $(show__cat).fadeIn();
    });
  });
</script>


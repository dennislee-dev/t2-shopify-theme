<style>
  .tabbing_sec_container {
    max-width: 64em;
    width: 100%;
    margin: 0 auto;
  }
  .tabbs_titles_holder {
    display: flex;
    flex-wrap: wrap;
    justify-content: center;
    gap: 100px;
    cursor: pointer;
    transition: all 0.4s ease;
  }
  .tabbs_titles_holder h5 {
    transition: all 0.4s ease;
    color: #9b9b9b;
    font-size: 10px;
    line-height: 15px;
  }
  .tabbs_titles_holder h5:hover,
  .tabbs_titles_holder h5.active_heading {
    color: #dd4127;
  }
  .tabbing__data__holder h5 {
    color: #353535;
    font-size: 12px;
    line-height: 15px;
    margin: 10px 0;
  }
  .tabbing__data__holder a {
    color: #9b9b9b;
    font-size: 10px;
    line-height: 15px;
    text-decoration: none;
    transition: all 0.4s ease;
    margin: 5px 0;
    text-transform: uppercase;
  }
  .tabbing__data__holder {
    padding-top: 40px;
    margin-bottom: 50px;
  }
  .tabbing__data__holder a:hover {
    color: #dd4127;
  }
  .characters__links {
    display: flex;
    flex-wrap: wrap;
    flex-direction: column;
  }
  .tabbing__list__area {
    columns: 5;
  }
  .inn__mobile {
    display: none;
  }
  /* ======== media quries ===== */
  @media (max-width: 830px) {
    .tabbing_sec_container {
      max-width: 100%;
      width: 100%;
      padding: 0 20px;
    }
    .inn__mobile {
      display: block;
    }
    .inn__desktop {
      display: none;
    }
    .inn__mobile .tabbs_titles_holder {
      row-gap: 16px;
      justify-content: center;
      column-gap: 0;
    }
    .inn__mobile .tabbs_titles_holder h5 {
      width: 25%;
      text-align: center;
    }
    .inn__mobile .tabbing__list__area:not(:first-child) {
      display: none;
    }
    .inn__mobile .tabbing__list__area {
      columns: 1;
      text-align: center;
    }
    .inn__mobile .tabbing__list__area a {
      margin: 15px 0;
    }
  }
</style>

<section class="tabbing__wrapper">
  <div class="tabbing_sec_container">
    <div class="inn__desktop">
      <div class="tabbs_titles_holder">
        {% for block in section.blocks %}
          {% if block.type == 'block-1' %}
            <h5
              class="tab__tile {% if forloop.index == 1 %}active_heading{% endif %}"
              data-targe="#{{ block.settings.access }}"
            >
              {{ block.settings.tbl }}
            </h5>
          {% endif %}
        {% endfor %}
      </div>
      <div class="tabbing__data__holder">
        {% for block in section.blocks %}
          {% if block.type == 'block-2' %}
            <div class="tabbing__list__area" id="{{ block.settings.accex }}" style="display: none;">
